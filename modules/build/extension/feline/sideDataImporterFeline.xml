<textFilePopulator>
    <columnDelimiterRegex>\t</columnDelimiterRegex>

	<textFileColumn>
		<identifier>true</identifier>
		<header>sequenceID</header>
		<property>sequenceID</property>
	</textFileColumn>
	
	<textFileColumn>
		<header>host_sci_name</header>
		<property>isolate_data.host_sci_name</property>
		<nullRegex>-</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>		
	</textFileColumn>

    <textFileColumn>
        <header>host_common_name</header>
        <property>isolate_data.host_common_name</property>
        <nullRegex>-</nullRegex>
        <overwriteExistingNonNull>true</overwriteExistingNonNull>
    </textFileColumn>

	<textFileColumn>
		<header>country</header>
		<property>isolate_data.country</property>
		<nullRegex>-</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>

    <textFileColumn>
        <header>place_sampled</header>
        <property>isolate_data.place_sampled</property>
        <nullRegex>-</nullRegex>
        <overwriteExistingNonNull>true</overwriteExistingNonNull>
    </textFileColumn>

	<textFileColumn>
		<header>collection_year</header>
		<property>isolate_data.collection_year</property>
		<nullRegex>-</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull> <!-- will not update to NULL --> 		
	</textFileColumn>

	<textFileColumn>
		<header>genotype</header>
		<property>genotype</property>
		<nullRegex>-</nullRegex>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
	</textFileColumn>
	
	<textFileColumn>
		<header>subtype</header>
		<property>subtype</property>
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
		<nullRegex>-</nullRegex>
	</textFileColumn>

    
</textFilePopulator>
